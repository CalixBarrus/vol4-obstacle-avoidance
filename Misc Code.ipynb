   "source": [
    "def SAS_height(a, b, theta):\n",
    "    c = (a**2 + b**2 - 2*a*b*np.cos(theta))**.5\n",
    "    A = a*b*np.sin(theta)/2\n",
    "    height = 2*A/c\n",
    "    return height"
   ]
